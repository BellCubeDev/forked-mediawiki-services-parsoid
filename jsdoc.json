{
    "opts": {
        "encoding": "utf8",
        "destination": "docs",
        "package": "package.json",
        "readme": "README.md",
        "recurse": true,
        "template": "node_modules/jsdoc-wmf-theme",
        "tutorials": "guides"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [ "lib/api", "lib/config", "lib/logger" ],
        "includePattern": ".+\\.js$"
    },
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "default": {
            "useLongnameInNav": true
        },
        "wmf": {
            "maintitle": "Parsoid",
            "repository": "https://github.com/wikimedia/parsoid"
        }
    }
}
